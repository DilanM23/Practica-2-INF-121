package Ejercicio_3;

public class Estudiante extends Persona{
	private int ru;
	private String fecha_ingreso;
	private String semestre;
	
	public Estudiante() {
		super();
		this.fecha_ingreso="23/02/2024";
		this.semestre="4to.";
	}
	public void mostrar() {
		super.mostrar();
		System.out.println("<ESTUDIANTE>");
		System.out.println("R.U.: "+this.ru);
		System.out.println("Fecha Ingreso: "+this.fecha_ingreso);
		System.out.println("Semestro: "+this.semestre);
	}
		
}
