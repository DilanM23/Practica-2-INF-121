package Ejercicio_3;

public class Estudiante extends Persona{
	private int ru;
	private String fecha_ingreso;
	private String semestre;
	
	public Estudiante() {
		super();
		this.fecha_ingreso="23/02/2024";
		this.semestre="4to.";
	}
	public Estudiante(int cix, String nombrex, String apellidox, int celularx, String fecha_nacx, int rux, String fecha_ingresox, String semestrex ) {
		super(cix, nombrex, apellidox, celularx, fecha_nacx);
		this.fecha_ingreso=fecha_ingresox;
		this.semestre=semestrex;
	}
	public void mostrar() {
		super.mostrar();
		System.out.println("<ESTUDIANTE>");
		System.out.println("R.U.: "+this.ru);
		System.out.println("Fecha Ingreso: "+this.fecha_ingreso);
		System.out.println("Semestre: "+this.semestre);
		System.out.println();
	}
		
}
